{
  "version": 2,
  "builds": [
    {
      "src": "api/image-to-text.js",
      "use": "@vercel/node"
    },
    {
      "src": "pages/**/*.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/image-to-text",
      "dest": "/api/image-to-text.js",
      "methods": ["POST"]
    },
    {
      "src": "/(.*)",
      "dest": "/pages/$1"
    }
  ]
}
